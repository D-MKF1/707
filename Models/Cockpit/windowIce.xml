<PropertyList>
 <path>windowIce.ac</path>

	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>windowIceCapt</object-name>
		<object-name>windowIceCapt.FO</object-name>
		<object-name>windowIceFO</object-name>
		<object-name>windowIceFO.Capt</object-name>
  </effect>
  
	<animation>
		<type>select</type>
		<object-name>windowIceCapt</object-name>
		<object-name>windowIceCapt.FO</object-name>
		<condition>		
			<less-than>
				<property>sim/multiplay/generic/float[11]</property>
				<value>0.9</value>
			</less-than>
		</condition>
	</animation>
  
	<animation>
		<type>select</type>
		<object-name>windowIceFO</object-name>
		<object-name>windowIceFO.Capt</object-name>
		<condition>		
			<less-than>
				<property>sim/multiplay/generic/float[10]</property>
				<value>0.9</value>
			</less-than>
		</condition>
	</animation>
	 
	<!-- the ice effect -->
	<animation>
		<type>blend</type>
		<object-name>windowIceCapt</object-name>
		<object-name>windowIceCapt.FO</object-name>
		<property>sim/multiplay/generic/float[11]</property>
		<factor>1</factor>
	</animation> 

	<animation>
		<type>blend</type>
		<object-name>windowIceFO</object-name>
		<object-name>windowIceFO.Capt</object-name>
		<property>sim/multiplay/generic/float[10]</property>
		<factor>1</factor>
	</animation> 
	
	<!-- go inside -->
	<animation>
	<type>rotate</type>
		<object-name>windowIceCapt.FO</object-name>
		<property>sim/multiplay/generic/float[5]</property>
		<interpolation>
			<entry><ind>0.0</ind><dep> 0.0</dep></entry>
			<entry><ind>0.1</ind><dep> 0.0</dep></entry>
			<entry><ind>0.2</ind><dep>-4</dep></entry>
			<entry><ind>1.0</ind><dep>-4</dep></entry>
		</interpolation>
		<axis>
      <x1-m> -18.812</x1-m>
      <y1-m>   0.751</y1-m>
      <z1-m>   0.955</z1-m>
      <x2-m> -18.805</x2-m>
      <y2-m>   0.745</y2-m>
      <z2-m>   0.973</z2-m>
		</axis>
	</animation>
	<!-- slipping back 	-->
	<animation>
	<type>translate</type>
		<object-name>windowIceCapt.FO</object-name>
		<property>sim/multiplay/generic/float[5]</property>
		<interpolation>
			<entry><ind>0.0</ind><dep> 0.0</dep></entry>
			<entry><ind>0.2</ind><dep> 0.0</dep></entry>
			<entry><ind>1.0</ind><dep>-0.5</dep></entry>
		</interpolation>
    <axis>   
      <x1-m> -17.8</x1-m>
      <y1-m>   1.23</y1-m>
      <z1-m>   0.70</z1-m>
      <x2-m> -18.805</x2-m>
      <y2-m>   0.80</y2-m>
      <z2-m>   0.973</z2-m>
    </axis>
	</animation>

	<animation>
	<type>rotate</type>
		<object-name>windowIceCapt.FO</object-name>
		<property>sim/multiplay/generic/float[5]</property>
		<interpolation>
			<entry><ind>0.0</ind><dep> 0.0</dep></entry>
			<entry><ind>0.1</ind><dep> 0.0</dep></entry>
			<entry><ind>0.2</ind><dep> 0.01</dep></entry>
			<entry><ind>1.0</ind><dep>   8</dep></entry>
		</interpolation>
		<axis>
      <x1-m> -17.8</x1-m>
      <y1-m>   1.23</y1-m>
      <z1-m>   0.70</z1-m>
      <x2-m> -18.805</x2-m>
      <y2-m>   0.78</y2-m>
      <z2-m>   0.973</z2-m>
		</axis>
	</animation>
	
   	<!-- go inside -->
   	<animation>
   	<type>rotate</type>
   		<object-name>windowIceFO.Capt</object-name>
   		<property>sim/multiplay/generic/float[4]</property>
   		<interpolation>
   			<entry><ind>0.0</ind><dep> 0.0</dep></entry>
   			<entry><ind>0.1</ind><dep> 0.0</dep></entry>
   			<entry><ind>0.2</ind><dep> 4</dep></entry>
   			<entry><ind>1.0</ind><dep> 4</dep></entry>
   		</interpolation>
   		<axis>
         <x1-m> -18.812</x1-m>
         <y1-m>  -0.751</y1-m>
         <z1-m>   0.955</z1-m>
         <x2-m> -18.805</x2-m>
         <y2-m>  -0.745</y2-m>
         <z2-m>   0.973</z2-m>
   		</axis>
   	</animation>
   	<!-- slipping back 	-->
   	<animation>
   	<type>translate</type>
   		<object-name>windowIceFO.Capt</object-name>
   		<property>sim/multiplay/generic/float[4]</property>
   		<interpolation>
   			<entry><ind>0.0</ind><dep> 0.0</dep></entry>
   			<entry><ind>0.2</ind><dep> 0.0</dep></entry>
   			<entry><ind>1.0</ind><dep>-0.5</dep></entry>
   		</interpolation>
       <axis>   
         <x1-m> -17.8</x1-m>
         <y1-m>  -1.23</y1-m>
         <z1-m>   0.70</z1-m>
         <x2-m> -18.805</x2-m>
         <y2-m>  -0.80</y2-m>
         <z2-m>   0.973</z2-m>
       </axis>
   	</animation>

   	<animation>
   	<type>rotate</type>
   		<object-name>windowIceFO.Capt</object-name>
   		<property>sim/multiplay/generic/float[4]</property>
   		<interpolation>
   			<entry><ind>0.0</ind><dep> 0.0</dep></entry>
   			<entry><ind>0.1</ind><dep> 0.0</dep></entry>
   			<entry><ind>0.2</ind><dep> 0.01</dep></entry>
   			<entry><ind>1.0</ind><dep>  -8</dep></entry>
   		</interpolation>
   		<axis>
         <x1-m> -17.8</x1-m>
         <y1-m>  -1.23</y1-m>
         <z1-m>   0.70</z1-m>
         <x2-m> -18.805</x2-m>
         <y2-m>  -0.78</y2-m>
         <z2-m>   0.973</z2-m>
   		</axis>
   	</animation>
 
 </PropertyList>
