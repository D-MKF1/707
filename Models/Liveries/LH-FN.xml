<PropertyList>
<sim>
    <model>
        <livery>
            <name type="string">Lufthansa-Friedrichshafen</name>
            <texture>Liveries/LH-FN.png</texture>
            <texture-container>Liveries/LH-FN-container.png</texture-container>
        </livery>
    </model>
    <multiplay>
    	<generic>
    		<int n="8">0</int>   <!-- engine type -->
    		<int n="9">1</int>   <!-- cargo aircraft -->
    		<int n="16">3</int>  <!-- immat position -->
			<!-- 0 no immat, 1 on fuselage down, 2 on fuselage upper back, 3 on rudder and wings --> 
    	</generic>
    </multiplay>
</sim>
</PropertyList>
